{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "foodHour": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/foodHour",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/bootstrap.css",
              "src/assets/css/animate.min.css",                                                        
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/owl.theme.default.css",
              "src/assets/css/theme-blog.css",
              "src/assets/css/theme.css",
              "src/assets/css/theme-elements.css",
              "src/assets/css/settings.css",
              "src/assets/css/demo-one-page-agency.css",
             
              "src/styles.css"
            ],

            "scripts": [
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/modernizr.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/jquery.appear.min.js",
              "src/assets/js/jquery.easing.min.js",
             
              "src/assets/js/jquery-cookie.min.js",              
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/theme.js",
              "src/assets/js/custom.js",
              "src/assets/js/theme.init.js",
              "src/assets/js/jquery.themepunch.tools.min.js",
              "src/assets/js/jquery.themepunch.revolution.min.js",
              "src/assets/js/examples.gallery.js"
            ]   
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "foodHour:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "foodHour:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "foodHour:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "../src/assets/js/jquery.appear.min.js",
              "../src/assets/js/jquery.easing.min.js",
              "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"
              
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "foodHour-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "foodHour:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "foodHour"
}
